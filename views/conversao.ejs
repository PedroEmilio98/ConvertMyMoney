                    <h1>Dolares: <%= dolares %>
                    </h1>
                    <h1>Valor de venda: <%= resultadoVenda %>
                    </h1>
                    <h1>Valor de compra: <%= resultadoCompra %>
                    </h1>
                    <% } %>

                        <%- include('footer') %>