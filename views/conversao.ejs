                <h1>Cotacao: <%= cotacaoManual %>
                </h1>
                <h1>Dolares: <%= dolares %>
                </h1>
                <h1>Valor: <%= resultadoManual %>
                </h1>
                <% }else{ %>

                    <h1>Cotacao para venda: <%= cotacaoVenda %>
                    </h1>
